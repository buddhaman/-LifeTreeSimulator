# AI Provider Configuration
# Set GREENPT=true to use GreenPT, otherwise uses OpenAI
VITE_GREENPT=false

# OpenAI API Key (used when GREENPT=false)
# Get your API key from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Gemini API Key for generating images
# Get your API key from: https://aistudio.google.com/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here
#
# GreenPT API Key (used when GREENPT=true)
# Get your API key from: https://api.greenpt.ai
VITE_GREENPT_API_KEY=your_greenpt_api_key_here
